#include<iostream>
using namespace std;
void hollow_star_pyramid()
{
	int i,j,k,rows=10;
	for(i=1;i<=rows;i++)
	{
		for(j=1;j<=rows-i;j++)
		{
			cout<<" ";
		}
		for(k=1;k<=2*i-1;k++)
		{
			if(k==1||k==2*i-1||i==rows)
			cout<<"*";
			else
			cout<<" ";
		}
		cout<<endl;
	}
}
int main()
{
	hollow_star_pyramid();
}
