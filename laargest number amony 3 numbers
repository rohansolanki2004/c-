#include<iostream>
using namespace std;
float larget_number(float a, float b ,float c)
{
	cout<<"enter the 1st number ";
	cin>>a;
	cout<<"enter the 2nd number ";
	cin>>b;
	cout<<"enter the 3rd number ";
	cin>>c;
	if(a>b && a>c)
	cout<<a<<"is largest ";
	else if (b>c && b>a)
	cout<<b<<"is largest";
	else 
	cout<<c<<"is largest";
}
using namespace std;
int main()
{
	float a,b,c;
	larget_number(a,b,c);
}
