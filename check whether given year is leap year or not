#include<iostream>
using namespace std;
void leap_year(int year)
{
	if(year%400==0)
	cout<<"year is leap year";
	else if(year%100 ==0)
	cout<<"year is not leap year ";
	else if(year%4 ==0)
	cout<<"year is leap year";
	else 
	cout<<"year is not leap year ";
	
}
int main()
{
	int year;
	cout<<"enter year :";
	cin>> year;
	leap_year(year);
	return 0 ;
}
