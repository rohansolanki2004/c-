#include<iostream>
#include<cmath>
using namespace std;
int armstrong(int a)
{
	int cube_sum=0;
	while(a>0)
	{
		cube_sum=cube_sum+ pow(a%10,3);
		a=a/10;
	}
	return (cube_sum);
}
int main()
{
	int a=153,arm;
	arm=armstrong(a);
	if(arm==a)
	cout<<a<<" is armstrong number";
	else
	cout<<a<<" is not armstrong number ";
	return 0;
	
}
